{
    "compilerOptions": {
        "lib": [
            "es5",
            "es6",
            "es2018",
            "dom"
        ],
        "strict": true,
        "target": "es3",
        "downlevelIteration": true,
        "noImplicitAny": true,
        "removeComments": true,	
        "sourceMap": true,
        "moduleResolution": "node",
        "esModuleInterop": true
    },
    "files": ["pret.ts"],
    "exclude": ["test_*.ts", "node_modules", "**/*.spec.ts"]
}
